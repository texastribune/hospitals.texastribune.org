<header class="select">
  <div class='title'>
    <h2>Compare Hospitals</h2>
  </div>
  <div class="controls">
    <a href="#" class="button compare">Select Different Hospitals to Compare</a>
  </div>
  <div class="instructions">
    <p>Compare the hospitals you've selected before.</p>
  </div>
</header>
<section>
  <article>
    <h2>General Information</h2>
    <table>
      <thead>
        <th></th>
        <% _(children).each(function(model) { %><th><%= model.name %></th><% }); %>
      </thead>
      <tbody>
        <tr>
          <td>Address</td>
          <% _(children).each(function(model) { %>
          <td data-title="<%= model.name %>"><%= model.address %></td>
          <% }); %>
        </tr>
        <tr>
          <td>Phone Number</td>
          <% _(children).each(function(model) { %>
          <td data-title="<%= model.name %>"><%= model.phone_number %></td>
          <% }); %>
        </tr>
        <tr>
          <td>Type</td>
          <% _(children).each(function(model) { %>
          <td data-title="<%= model.name %>"><%= model.type %></td>
          <% }); %>
        </tr>
        <tr>
          <td>Ownership</td>
          <% _(children).each(function(model) { %>
          <td data-title="<%= model.name %>"><%= model['hospital ownership'] %></td>
          <% }); %>
        </tr>
        <tr>
          <td>Emergency Services?</td>
          <% _(children).each(function(model) { %>
          <td data-title="<%= model.name %>"><%= model.emergency_services %></td>
          <% }); %>
        </tr>
      </tbody>
    </table>
  </article>
  <article>
    <h2>Emergency Care</h2>
    <table>
      <thead>
        <th>Average wait time</th>
        <% _(children).each(function(model) { %><th><%= model.name %></th><% }); %>
      </thead>
      <tbody>
        <tr>
          <td>Before being seen by a healthcare professioanl</td>
          <% _(children).each(function(model) { %>
          <td data-title="<%= model.name %>"><%= model.data.emergency_rates.ed1 %></td>
          <% }); %>
        </tr>
        <tr>
          <td>Before being admitted to the hospital</td>
          <% _(children).each(function(model) { %>
          <td data-title="<%= model.name %>"><%= model.data.emergency_rates.op_18 %></td>
          <% }); %>
        </tr>
        <tr>
          <td>Before getting a diagnostic</td>
          <% _(children).each(function(model) { %>
          <td data-title="<%= model.name %>"><%= model.data.emergency_rates.door_to_diagnostic_eval %></td>
          <% }); %>
        </tr>
      </tbody>
    </table>
  </article>
